{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_PATH": "/tmp/prezenty.db",
    "BACKUP_PATH": "/tmp/prezenty-backup.db"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
} 